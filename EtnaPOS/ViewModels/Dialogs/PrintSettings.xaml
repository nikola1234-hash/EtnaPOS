<Window x:Class="EtnaPOS.ViewModels.Dialogs.PrintSettings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EtnaPOS.ViewModels.Dialogs"
        mc:Ignorable="d"
        Title="PrintSettings" Height="200" Width="400">
    <Grid>
        <StackPanel Width="150" Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center" >
            <StackPanel Orientation="Horizontal">
                <Label Content="FontSize"></Label>
                <TextBox Width="80" x:Name="fontSize"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Width"></Label>
                <TextBox Width="80" x:Name="layoutWidth"></TextBox>
            </StackPanel>
            <StackPanel Orientation="Horizontal">
                <Label Content="Height"></Label>
                <TextBox Width="80" x:Name="layoutHeight"></TextBox>
            </StackPanel>
            <Button Content="Save" Click="ButtonBase_OnClick" Margin="0 20 0 0"></Button>
            <Button Content="PrintTest" Click="ButtonBase_OnClick2" Margin="0 20 0 0"></Button>
        </StackPanel>
    </Grid>
</Window>
