<dx:ThemedWindow
    x:Class="EtnaPOS.ViewModels.Dialogs.SelectPrinterDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    Title="Opcije stampe" Height="200" Width="400" WindowStartupLocation="CenterScreen" ResizeMode="NoResize"
    >

    <Grid>
        <StackPanel Width="150" VerticalAlignment="Center" HorizontalAlignment="Center" Orientation="Vertical">
            <Label Content="Izaberi stampac za slipove"></Label>
            <ComboBox x:Name="comboBox"></ComboBox>
            <Button Content="Izaberi" Click="ButtonBase_OnClick" Margin="0 30 0 0">
                
            </Button>
        </StackPanel>
    </Grid>
</dx:ThemedWindow>
